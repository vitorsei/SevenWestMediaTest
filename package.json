{
  "name": "SevenWestMediaTest",
  "version": "0.1.0",
  "description": "Seven West Media Test",
  "main": "./dist/bundle.js",
  "dependencies": {
    "expose-loader": "^0.7.3",
    "html-webpack-plugin": "^2.28.0",
    "node-static": "^0.7.9",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "styled-components": "^3.1.6"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.5",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/html-webpack-plugin": "^2.28.0",
    "@types/jest": "21.1.8",
    "@types/node": "^8.0.9",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/react-hot-loader": "^3.0.3",
    "@types/webpack": "^3.0.2",
    "@types/webpack-env": "^1.13.0",
    "awesome-typescript-loader": "^3.1.3",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "jest": "21.1.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "source-map-loader": "^0.2.1",
    "styled-components-ts": "0.0.8",
    "ts-jest": "21.0.1",
    "ts-node": "^3.2.0",
    "tslib": "1.7.1",
    "typescript": "^2.3.3",
    "typescript-eslint-parser": "8.0.0",
    "webpack": "^2.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server",
    "static": "static -at 0.0.0.0 dist",
    "test": "jest",
    "test:build": "npm run build && npm run lint",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.ts",
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testURL": "http://localhost:1234",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitorsei/SevenWestMediaTest.git"
  },
  "author": "Vitor Iwamura"
}
